<div class="container">
  <div class="page-header">
    <h1>Our Team</h1>
  </div>
  
    <% @agents.each do |agent| %>
      <div class="row agent">
        <div class="col-sm-4 agent-picture">
          <img src="#" style="width: 100%; height:100%" />
        </div>

        <div class="col-sm-8 agent-description">
          <h1><%= agent.to_s%></h1>
          <div class="well">
            <p><%= agent.description %></p>
          </div>
          <p><%= agent.email %></p>
          <p><%= number_to_phone(agent.phone, country_code: 1) %></p>
          <a class="btn btn-large btn-default" href="#">Contact Me</a>
        </div>
      </div>
    <% end %>
    
    
    
  



</div>